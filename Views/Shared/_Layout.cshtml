@using Microsoft.AspNetCore.Identity;
@using TechTales.Data.Models;
@using TechTales.Helpers.Extensions
@inject UserManager<UserEntity> UserManager;

@{
    var user = await UserManager.GetUserAsync(User);
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>TechTales - @ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/reset.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/resetbootstrap.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/layout/layout.css">
    <link rel="stylesheet" href="~/TechTales.styles.css" asp-append-version="true" />
</head>
<body class="">
    <header class="bg-blueviolet">
        <nav class="navbar navbar-expand-md navbar-dark">
            <div class="container d-flex flex-md-row justify-content-between align-items-center py-2" style="column-gap: 12px;">
                
                <a asp-controller="Home" asp-action="Index" class="mx-2 text-white d-none d-md-inline-block">Home</a>
                <a asp-controller="Blog" asp-action="List" asp-route-orderBy="date" class="mx-2 text-white d-none d-md-inline-block">All Blogs</a>
                <a asp-controller="Home" asp-action="About" class="mx-2 text-white d-none d-md-inline-block">About us</a>
                <a asp-controller="Home" asp-action="Contacts" class="mx-2 text-white d-none d-md-inline-block">Contacts</a>

                <div class="mx-2 flex-grow-1">
                    <input type="text" placeholder="Search" class="form-control search-input d-none d-md-inline-block" value="@ViewBag.Request">
                </div>

                @{
                    if (user is null)
                    {
                        <a asp-controller="Authorization" asp-action="Login" class="mx-2  text-white d-none d-md-inline-block">Log in</a>
                        <a asp-controller="Authorization" asp-action="Signup" class="mx-2  text-white d-none d-md-inline-block">Sign up</a>
                    }
                    else 
                    {
                        <div class="dropdown">
                            <a class="nav-link dropdown-toggle d-flex align-items-center" href="#" id="userDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                <img src="@user.Avatar.BlobToImageSrc()"
                                    class="rounded-circle dropbtn" style="width: 40px; height: 40px; object-fit: cover;" />
                                <span class="ms-3 me-2 lead text-white">@user.UserName!.EllipsisString(20)</span>
                            </a>
                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="userDropdown">
                                <li><a class="dropdown-item" asp-action="Detail" asp-controller="Profile" asp-route-id="@user.Id">Profile</a></li>
                                <li><a class="dropdown-item" href="/Settings">Settings</a></li>
                                <li><hr class="dropdown-divider"></li>
                                <li>
                                    <form id="logout-form" asp-action="Logout" asp-controller="Authorization" method="post" style="display:none;">
                                        @Html.AntiForgeryToken()
                                    </form>
                                    <a href="#" class="dropdown-item" onclick="document.getElementById('logout-form').submit();">Logout</a>
                                </li>
                            </ul>
                        </div>
                    }
                }
            
            </div>
        </nav>
    </header>

